{
    "compilerOptions": {
        "noImplicitAny": true,
        "strictNullChecks": true,
        "outDir": "_site",
        "target": "ESNext",
        "moduleResolution": "node",
    },
    "include": [
        "_src/*",
        "_src/models/*",
        "_src/controllers/*",
        "_src/assets/js/*",
    ],
    "exclude": [
        "node_modules"
    ],
}